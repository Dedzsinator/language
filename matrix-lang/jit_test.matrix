// Test JIT compilation with mathematical functions
fn square(x: Float) -> Float {
    x * x
}

fn add(a: Float, b: Float) -> Float {
    a + b
}

fn factorial(n: Int) -> Int {
    if n <= 1 then 1 else n * factorial(n - 1)
}

// Test the functions
let x = 5.0;
let y = 3.0;
let result = add(square(x), square(y));
result
