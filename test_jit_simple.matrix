-- Test basic arithmetic functions suitable for JIT

-- Simple arithmetic functions
let add_ints = (a: Int, b: Int) => a + b
let multiply = (x: Int, y: Int) => x * y
let power_func = (base: Int, exp: Int) => base ^ exp

-- Test JIT-compiled function calls
