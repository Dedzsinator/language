-- Simple test for function syntax
-- Testing different function definition styles

let simple = 5

-- Function without explicit return type
let add1 = (a, b) => a + b

-- Function with parameter types
let add2 = (a: Int, b: Int) => a + b

-- Function with return type
let add3: (Int, Int) -> Int = (a, b) => a + b

-- Test result
let result = add1(2, 3)
